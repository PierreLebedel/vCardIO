{
    "name": "pleb/vcardio",
    "description": "Parse & write vCard (vcf) files",
    "keywords": [
        "vcard",
        "vcf"
    ],
    "homepage": "https://github.com/pleb/vcardio",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre Lebedel",
            "email": "pierrelebedel@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laravel/pint": "^v1.0",
        "pestphp/pest": "^v2.0|^v3.0",
        "symfony/var-dumper": "^v6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Pleb\\VCardIO\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pleb\\VCardIO\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
